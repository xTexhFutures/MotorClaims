            </div>

            <div style="background-color: white;min-height: 3px;"></div>
            <div class="inner-clild-wrap">
                <ul class="detail-list">

                    <li class="detailed-item false">
                        <span class="detailed-icon-wrap">
                            <img src="/assets/icons/save.svg" alt="" class="item-icon">
                        </span>
                        <div class="item-content">
                            <span class="label">Color</span>
                            <h3 class="value ellipsis">@item.ColorDesc</h3>
                        </div>
                    </li>


                    <li class="detailed-item false">
                        <span class="detailed-icon-wrap">
                            <img src="/assets/icons/calendar.svg" alt="" class="item-icon">
                        </span>
                        <div class="item-content">
                            <span class="label">Plate Number</span>
                            <h3 class="value ellipsis">@item.PlateNo</h3>
                        </div>
                    </li>
                    <li class="detailed-item false">
                        <span class="detailed-icon-wrap">
                            <img src="/assets/icons/save.svg" alt="" class="item-icon">
                        </span>
                        <div class="item-content">
                            <span class="label">Chassis No</span>
                            <h3 class="value ellipsis">@item.ChassisNo</h3>
                        </div>
                    </li>

                </ul>
            </div>
        </div>
    </div>

    
